{
  "name": "The WishWiz",
  "version": "1.0.0",
  "author": "Burli & Moon36",
  "manifest_version": 2,
  "description": "A extension that finds all the free items of your query on the awesome marketplace wish.com",
  "homepage_url": "https://burli.biz/wishwiz",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "src/options.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "The WishWiz",
    "default_popup": "src/popup.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}
